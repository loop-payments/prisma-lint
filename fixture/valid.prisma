generator client {
  provider        = "prisma-client-js"
  output          = "../generated/prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = "fake-url"
}

model DbUser {
  id         String @id
  createdAt  String @map(name: "created_at")
  @@map(name: "user")
}

model DbTenant {
  /// prisma-lint-ignore-model required-fields createdAt
  id         String @id
  @@map(name: "tenant")
}
