generator client {
  provider        = "prisma-client-js"
  output          = "../generated/prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = "fake-url"
}

model Users {
  id         String @id
  @@map(name: "users")
}

model UserRoleFoo {
  id         String @id
  @@map(name: "unexpected_snake_case")
}

model UserRole {
  id         String @id
  // No mapping.
}
